<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>fr.mitic.master2.server.GreetingServiceImpl</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>servcieServlet</servlet-name>
    <servlet-class>fr.mitic.master2.server.Services</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/tpgwt/greet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>servcieServlet</servlet-name>
    <url-pattern>/tpgwt/service</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>tpgwt.html</welcome-file>
  </welcome-file-list>

</web-app>
